{
  "name": "changelog",
  "version": "1.0.5",
  "description": "Command line tool (and Node module) that generates a changelog in color output, markdown, or json for modules in npmjs.org's registry as well as any public github.com repo.",
  "keywords": [
    "changelog",
    "change log",
    "commit messages",
    "commits",
    "changes",
    "history",
    "what's new",
    "change set"
  ],
  "preferGlobal": true,
  "homepage": "http://github.com/dylang/changelog",
  "author": "Dylan Greene <dylang@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylang/changelog.git"
  },
  "bugs": {
    "mail": "dylang@gmail.com",
    "url": "http://github.com/dylang/changelog/issues"
  },
  "directories": {
    "lib": "./lib",
    "bin": "./bin"
  },
  "main": "lib/changelog",
  "bin": {
    "changelog": "./bin/changelog.js"
  },
  "dependencies": {
    "request": "*",
    "wordwrap": "*",
    "cli": "*",
    "semver": "*",
    "has-color": "~0.1.1",
    "q": "~1.0.0",
    "chai": "~1.8.1",
    "chalk": "~0.4.0",
    "moment": "~2.5.0"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "mocha": "~1.17.0",
    "proxyquire": "~0.5.1",
    "grunt": "~0.4.1",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-notify": "~0.2.13",
    "grunt-contrib-watch": "~0.5.3",
    "time-grunt": "~0.2.9",
    "grunt-mocha-test": "~0.9.0",
    "load-grunt-tasks": "~0.3.0",
    "grunt-readme": "~0.4.5",
    "grunt-templates-dylang": "~0.1.3"
  },
  "scripts": {
    "test": "grunt test"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/dylang/changelog/raw/master/LICENSE"
    }
  ]
}
